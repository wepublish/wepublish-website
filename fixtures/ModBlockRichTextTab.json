[
  {
    "id": "ModBlockRichTextTab",
    "target": "{{entityEntity}}",
    "definition": {
      "key": "blockRichTextTab",
      "model": {
        "struct": {
          "blockName": {
            "string": {
              "maxLength": 40,
              "minLength": 3
            }
          },
          "list": {
            "list": {
              "elements": {
                "struct": {
                  "title": {
                    "string": {
                      "maxLength": 40,
                      "minLength": 3
                    }
                  },
                  "text": {
                    "struct": {
                      "blocks": {
                        "list": {
                          "elements": {
                            "any": {}
                          }
                        }
                      },
                      "entityMap": {
                        "map": {
                          "elements": {
                            "struct": {
                              "data": {
                                "union": {
                                  "document": {
                                    "ref": "{{ModelDocuments}}"
                                  },
                                  "navigationNode": {
                                    "ref": "{{ModelNavigationNode}}"
                                  },
                                  "url": {
                                    "string": {
                                      "regex": {
                                        "caseInsensitive": false,
                                        "expression": "^$|^([a-z][+.\\-a-z0-9]*):(//([^:]+:[^@]+@)?([^/:]+)(:[1-9][0-9]*)?)?/?([^?]+)(\\?[^#]+)?(#.*)?$",
                                        "multiline": false
                                      }
                                    }
                                  }
                                }
                              },
                              "mutability": {
                                "string": {}
                              },
                              "type": {
                                "string": {}
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              },
              "minLength": 1
            }
          },
          "backgroundColor": {
            "string": {
              "oneOf": [
                "turquoise",
                "black",
                "white",
                "grey"
              ]
            }
          },
          "anchorName": {
            "string": {
              "maxLength": 40,
              "minLength": 0
            }
          }
        }
      }
    }
  }
]