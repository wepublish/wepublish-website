[
  {
    "id": "ModelNavigationNode",
    "target": "{{entityEntity}}",
    "definition": {
      "key": "navigation-node",
      "model": {
        "struct": {
          "blockName": {
            "string": {
              "maxLength": 40,
              "minLength": 3
            }
          },
          "label": {
            "string": {}
          },
          "slug": {
            "string": {
              "regex": {
                "caseInsensitive": false,
                "expression": "^[a-z_\\-]*$",
                "multiline": false
              }
            }
          },
          "children": {
            "list": {
              "elements": {
                "ref": "recursive"
              },
              "maxLength": 200,
              "minLength": 0
            }
          },
          "showInMenu": {
            "bool": {}
          }
        }
      }
    }
  }
]