[
  {
    "id": "ModelArticle",
    "target": "{{entityEntity}}",
    "definition": {
      "key": "article",
      "model": {
        "struct": {
          "blockName": {
            "string": {
              "maxLength": 40,
              "minLength": 3
            }
          },
          "title": {
            "string": {
              "maxLength": 40,
              "minLength": 0
            }
          },
          "description": {
            "string": {
              "maxLength": 200,
              "minLength": 0
            }
          },
          "navigationNode": {
            "ref": "{{ModelNavigationNode}}"
          },
          "shareImage": {
            "ref": "{{ModelDocuments}}"
          },
          "isLive": {
            "bool": {}
          },
          "publishDate": {
            "float": {}
          },
          "expiryDate": {
            "float": {}
          },
          "tags": {
            "list": {
              "elements": {
                "string": {}
              },
              "maxLength": 100,
              "minLength": 0
            }
          },
          "contents": {
            "list": {
              "elements": {
                "union": {
                  "blockSpacer": {
                    "ref": "{{ModBlockSpacer}}"
                  },
                  "blockRichText": {
                    "ref": "{{ModBlockRichText}}"
                  },
                  "blockHeader": {
                    "ref": "{{ModBlockHeader}}"
                  },
                  "blockTitle": {
                    "ref": "{{ModBlockTitle}}"
                  },
                  "blockTeaserSpot": {
                    "ref": "{{ModBlockTeaserSpot}}"
                  },
                  "blockImageSlider": {
                    "ref": "{{ModBlockImageSlider}}"
                  }
                }
              },
              "maxLength": 100,
              "minLength": 0
            }
          }
        }
      }
    }
  }
]